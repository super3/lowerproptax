{
  "name": "lowerproptax-server",
  "version": "1.0.0",
  "description": "LowerPropTax backend server with Clerk authentication",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "migrate": "node-pg-migrate -m src/db/migrations",
    "migrate:up": "node-pg-migrate up -m src/db/migrations",
    "migrate:down": "node-pg-migrate down -m src/db/migrations",
    "migrate:create": "node-pg-migrate create -m src/db/migrations --migration-file-language cjs"
  },
  "keywords": [
    "lowerproptax",
    "property-management",
    "clerk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clerk/backend": "^1.19.4",
    "@clerk/express": "^1.6.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
